		super.onCreate(savedInstanceState);
		setContentView(R.layout.activity_main);
		
		Log.e("tag", "error log");
		Log.w("tag", "warning log");
		Log.i("tag", information log);
		Lod.d("tag", "debugging log");
		Log.v("tag", "verbose log");
	}
}
